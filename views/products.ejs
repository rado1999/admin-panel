<div class="column is-10" id="page-content">
    <div class="content-body">
        <div class="card">
            <div class="card-filter">
                <div class="field">
                    <div class="control has-icons-left has-icons-right">
                        <input class="input" id="table-search" type="text" placeholder="Search for records...">
                        <span class="icon is-left">
                            <i class="fa fa-search"></i>
                        </span>
                    </div>
                </div>
                <div class="field">
                    <div class="select">
                        <select id="table-length">
                            <option>10</option>
                            <option>25</option>
                            <option>50</option>
                        </select>
                    </div>
                </div>
                <div class="field has-addons">
                    <p class="control">
                        <a class="button" href="#">
                            <span class="icon is-small">
                                <i class="fa fa-plus"></i>
                            </span>
                            <span>Create Record</span>
                        </a>
                    </p>
                    <p class="control">
                        <a class="button" id="table-reload">
                            <span class="icon is-small">
                                <i class="fa fa-refresh"></i>
                            </span>
                            <span>Reload</span>
                        </a>
                    </p>
                </div>
            </div>
            <div class="card-content">
                <table class="table is-hoverable is-bordered is-fullwidth" id="datatable">
                    <thead>
                        <tr>
                            <th>ID</th>
                            <th>Image Urls</th>
                            <th>Title</th>
                            <th>Company</th>
                            <th>Logo</th>
                            <th>Model</th>
                            <th>Description</th>
                            <th>Price</th>
                            <th class="has-text-centered">Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% products.forEach(product => { %>
                            <tr>
                                <td><%= product.id %> </td>
                                <td><%= product.imageUrl %> </td>
                                <td><%= product.title %> </td>
                                <td><%= product.company %> </td>
                                <td><%= product.companyImage %> </td>
                                <td><%= product.model %> </td>
                                <td><%= product.mainDescription %> </td>
                                <td><%= product.price %> </td>
                                <td class="has-text-centered">
                                    <div class="field is-grouped action">
                                        <p class="control">
                                            <a href="countries/edit/1.html" class="button is-rounded is-text">
                                                <span class="icon">
                                                    <i class="fa fa-edit"></i>
                                                </span>
                                            </a>
                                        </p>
                                        <p class="control">
                                            <a class="button is-rounded is-text action-delete" data-id="1">
                                                <span class="icon">
                                                    <i class="fa fa-trash"></i>
                                                </span>
                                            </a>
                                        </p>
                                    </div>
                                </td>
                            </tr>
                        <% }) %>
                    </tbody>
                </table>
        </div>
    </div>
            </div>
        </div>

        <script src="js/app.js"></script>
            <script>
        $(document).ready(function() {
            var table = $('#datatable').DataTable({
                dom: "<'columns table-wrapper'<'column is-12'tr>><'columns table-footer-wrapper'<'column is-5'i><'column is-7'p>>"
            });

            $('#table-search').on('keyup', function() {
                let value = $(this).val();
                table.search(value).draw();
            });

            $('#table-length').on('change', function() {
                let value = $(this).val();
                table.page.len(value).draw();
            });

            $('#table-reload').on('click', function() {
                table.draw();
            });
        });
    </script>

